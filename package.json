        "css-loader": "^0.23.1",
        "express": "^4.13.3",
        "http-proxy": "^1.15.1",
        "http-proxy-middleware": "^0.17.2",
        "karma": "^1.3.0",
        "karma-coverage": "^1.1.1",
        "karma-mocha": "^1.2.0",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-sinon-chai": "^1.2.4",
