        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "babel-register": "^6.18.0",
        "better-npm-run": "0.0.11",
        "body-parser": "^1.14.2",
        "chai": "^3.5.0",
        "chromedriver": "^2.24.1",
        "compression": "^1.6.2",
        "connect-history-api-fallback": "^1.3.0",
        "cross-env": "^3.1.3",
