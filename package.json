{
  "name": "react-redux-univeral-with-django",
  "version": "0.0.2",
  "scripts": {
    "start": "better-npm-run start-prod",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-api\" ",
    "dev-without-api": " concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" ",
    "start-dev": "better-npm-run start-dev",
    "start-api": "better-npm-run start-api",
    "watch-client": "better-npm-run watch-client",
    "e2e": "node frontend/test/e2e/runner.js",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-api": {
      "command": "python manage.py runserver 0.0.0.0:3030"
    },
    "watch-client": {
      "command": "node webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "PORT": 3000,
        "APIPORT": 3030
      }
    }
  },
